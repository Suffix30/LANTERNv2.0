{
  "manifest_version": 3,
  "name": "Agent BLACK - Browser Bridge",
  "version": "1.0.0",
  "description": "Sends browser context to Agent BLACK for situational awareness",
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
